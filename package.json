    "@vue/eslint-config-airbnb": "^5.0.2",
    "@vue/test-utils": "^1.1.2",
    "@vuepress/plugin-google-analytics": "^1.2.0",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^22.0.4",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-vue": "^6.2.2",
